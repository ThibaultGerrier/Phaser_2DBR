{
    "rules":{
        "indent": ["error", 4],
        "max-len": ["error", { "code": 170, "tabWidth": 4 }],
        "no-console":0,
        "linebreak-style": "off",
        "import/no-unresolved": 0,
        "import/no-extraneous-dependencies": 0,
        "import/extensions": 0,
        "class-methods-use-this": 0,
        "no-underscore-dangle": 0
    },
    "plugins": [
        "react"
    ],
    "extends": [
        "airbnb/base",
        "plugin:react/recommended"
    ],
    "env": {
        "browser": true
    }
}
